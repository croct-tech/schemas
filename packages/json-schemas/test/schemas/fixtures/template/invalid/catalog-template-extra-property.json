{
  "schema": "template/catalog-template.json",
  "allErrors": true,
  "input": {
    "$schema": "https://schema.croct.com/json/template/template.json",
    "title": "Full template",
    "description": "A template with all properties filled.",
    "metadata": {
      "id": "shadcn-croct",
      "verified": true,
      "categories": ["ui", "component"],
      "ecosystem": {
        "name": "Croct",
        "avatarUrl": "https://cdn.croct.com/avatar.png",
        "websiteUrl": "https://croct.com"
      },
      "sourceUrl": "https://github.com/croct-tech/templates/shadcn-croct",
      "demoUrl": "https://demo.croct.com",
      "deployUrl": "https://deploy.croct.com",
      "coverImageUrl": "https://cdn.croct.com/image.png",
      "coverVideoUrl": "https://cdn.croct.com/video.mp4",
      "installationUrl": "shadcn://ui/component",
      "documentationUrl": "A documentation in markdown format.",
      "extra": true
    },
    "actions": [
      {
        "name": "add-component",
        "components": [
          "button"
        ]
      }
    ],
    "extra": true
  },
  "errors": [
    {
      "instancePath": "/metadata",
      "keyword": "additionalProperties",
      "message": "must NOT have additional properties",
      "params": {
        "additionalProperty": "extra"
      },
      "schemaPath": "#/properties/metadata/additionalProperties"
    },
    {
      "instancePath": "/metadata/id",
      "keyword": "pattern",
      "message": "must match pattern \"^[a-z0-9-]+/[a-z0-9-]+/[a-z0-9-]+$\"",
      "params": {
        "pattern": "^[a-z0-9-]+/[a-z0-9-]+/[a-z0-9-]+$"
      },
      "schemaPath": "#/properties/metadata/properties/id/pattern"
    },
    {
      "instancePath": "/metadata/documentationUrl",
      "keyword": "pattern",
      "message": "must match pattern \"^https://\"",
      "params": {
        "pattern": "^https://"
      },
      "schemaPath": "#/properties/metadata/properties/documentationUrl/pattern"
    },
    {
      "instancePath": "/metadata/documentationUrl",
      "keyword": "format",
      "message": "must match format \"uri\"",
      "params": {
        "format": "uri"
      },
      "schemaPath": "#/properties/metadata/properties/documentationUrl/format"
    },
    {
      "instancePath": "/metadata/categories/0",
      "keyword": "pattern",
      "message": "must match pattern \"^[a-z0-9-]+/[a-z0-9-]+$\"",
      "params": {
        "pattern": "^[a-z0-9-]+/[a-z0-9-]+$"
      },
      "schemaPath": "#/properties/metadata/properties/categories/items/pattern"
    },
    {
      "instancePath": "/metadata/categories/1",
      "keyword": "pattern",
      "message": "must match pattern \"^[a-z0-9-]+/[a-z0-9-]+$\"",
      "params": {
        "pattern": "^[a-z0-9-]+/[a-z0-9-]+$"
      },
      "schemaPath": "#/properties/metadata/properties/categories/items/pattern"
    }
  ]
}
