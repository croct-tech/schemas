{
  "schema": "template/action/start-server.json",
  "allErrors": true,
  "input": {
    "name": "",
    "server": {
      "script": "",
      "url": "",
      "arguments": []
    },
    "result": {
      "url": ""
    },
    "extra": true
  },
  "errors": [
    {
      "instancePath": "",
      "keyword": "additionalProperties",
      "message": "must NOT have additional properties",
      "params": {
        "additionalProperty": "extra"
      },
      "schemaPath": "#/additionalProperties"
    },
    {
      "instancePath": "/name",
      "keyword": "const",
      "message": "must be equal to constant",
      "params": {
        "allowedValue": "start-server"
      },
      "schemaPath": "#/properties/name/const"
    },
    {
      "instancePath": "/server/script",
      "keyword": "minLength",
      "message": "must NOT have fewer than 1 characters",
      "params": {
        "limit": 1
      },
      "schemaPath": "#/properties/server/properties/script/minLength"
    },
    {
      "instancePath": "/server/script",
      "keyword": "pattern",
      "message": "must match pattern \"^\\S+$\"",
      "params": {
        "pattern": "^\\S+$"
      },
      "schemaPath": "#/properties/server/properties/script/pattern"
    },
    {
      "instancePath": "/server/url",
      "keyword": "pattern",
      "message": "must match pattern \"^https?://.*\"",
      "params": {
        "pattern": "^https?://.*"
      },
      "schemaPath": "#/properties/server/properties/url/pattern"
    },
    {
      "instancePath": "/server/url",
      "keyword": "format",
      "message": "must match format \"uri\"",
      "params": {
        "format": "uri"
      },
      "schemaPath": "#/properties/server/properties/url/format"
    },
    {
      "instancePath": "/result/url",
      "keyword": "minLength",
      "message": "must NOT have fewer than 1 characters",
      "params": {
        "limit": 1
      },
      "schemaPath": "#/properties/result/properties/url/minLength"
    }
  ]
