{
  "schema": "template/action/locate-file.json",
  "input": {
    "name": "locate-file",
    "path": "src",
    "limit": 5,
    "depth": 3,
    "matcher": {
      "type": "or",
      "matchers": [
        {
          "type": "and",
          "matchers": [
            {
              "pattern": "export"
            }
          ]
        },
        {
          "type": "or",
          "matchers": [
            {
              "pattern": "export",
              "caseSensitive": true
            }
          ]
        }
      ]
    },
    "result": "files"
  },
  "errors": [
  ]
}
